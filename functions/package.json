{
  "name": "functions",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "setEnv:dev": "ts-node script/setEnvVars.ts dev",
    "setEnv:prod": "ts-node script/setEnvVars.ts prod"
  },
  "engines": {
    "node": "10"
  },
  "main": "build/index.js",
  "dependencies": {
    "firebase-admin": "^8.12.1",
    "firebase-functions": "^3.6.2"
  },
  "devDependencies": {
    "@types/node": "^14.0.6",
    "child_process": "^1.0.2",
    "firebase-functions-test": "^0.2.1",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3"
  },
  "private": true
}
